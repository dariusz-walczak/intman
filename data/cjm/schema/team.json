{
    "$schema": "http://json-schema.org/schema#",

    "type": "object",
    "properties": {
        "people": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "code": { "type": "string" },
                    "last name": { "type": "string" },
                    "first name": { "type": "string" },
                    "account id": { "type": "string" },
                    "daily capacity": {"type": "number"}
                },
                "required": ["code", "last name", "first name", "account id", "daily capacity"]
            }
        }
    },
    "required": ["people"]
}
